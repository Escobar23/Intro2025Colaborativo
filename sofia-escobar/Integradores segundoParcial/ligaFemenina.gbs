//Punto 1
function liga_SinSponsorLlamado_(liga, sponsor){
     /*
     Proposito:Describe la liga *liga* sin el sponsor *sponsor*, así como de todos aquellos equipos que pudieran tenerlo como sponsor.
     Precondicion: debe existir
     Tipo: LigaFemenina
     Obs:
     */
     sponsors:=sponsors(liga)
     nuevosSponsors:=[]
     equiposDeLaLiga_SinSponsorLlamado_(liga, sponsor)
     foreach sponsor in sponsors{
         nuevosSponsors:= nuevosSponsors ++ singular_Si_(sponsor, nombre(sponsor)/= sponsor)
     }
     return(LigaFemenina(sponsor <- nuevosSponsors, equipos <- equiposDeLaLiga_SinSponsorLlamado_(equipos(liga),sponsor)))

    return(LigaFemenina(
         sponsors <- nuevosSponsors,
         equipos <- equiposDeLaLiga_SinSponsorLlamado_(equipos(liga),sponsor)
     ))
 }
 
